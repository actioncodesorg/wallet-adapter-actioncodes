{
  "name": "@anthropic-test/wallet-adapter-actioncodes",
  "version": "0.1.0",
  "description": "Action Codes wallet adapter for Solana Wallet Adapter",
  "license": "Apache-2.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@actioncodes/sdk": "^2.0.18",
    "lit": "^3.2.1"
  },
  "peerDependencies": {
    "@solana/wallet-adapter-base": ">=0.9.0",
    "@solana/web3.js": "^1.98.0"
  },
  "devDependencies": {
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/web3.js": "^1.98.4",
    "happy-dom": "^20.5.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.0",
    "vitest": "^4.0.18"
  }
}
